pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'qopper pr1 Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'qopper pr1 Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'qopper pr1 Deploying....'
            }
        }
        stage('Next Job') {
            steps {
                build job: "test_multibranch2/master", parameters: [[$class: 'LabelParameterValue',name: 'NODE_NAME',value: 'label1']]
            }
        }
    }
}
